FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         backward;
}

gradSchemes
{
    default         Gauss linear;
}

divSchemes
{
      default         none;
      div(phi,U)      Gauss linear;
      div((nuEff*dev2(T(grad(U))))) Gauss linear;
//      div(phi,(0.5*magSqr(U))_0)    Gauss linear;
      div(phi,e)                  Gauss linear;
      div(phi,p)                  Gauss linear;
      div(phi,e_0)                  Gauss linear;
      div(phi,p_0)                  Gauss linear;
      div(transportVelocityPressureFieldMean) Gauss linear;
      div(transportTurbulenceFieldMean) Gauss linear;
      div(U)                  Gauss linear;      
}

laplacianSchemes
{
    default         Gauss linear corrected; // Gauss linear limited 1;
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         corrected;
}

wallDist
{
    method meshWave;
}


