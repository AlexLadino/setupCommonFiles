FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
// AVERAGING Co<1
      p
      {    
            solver           		GAMG;
            tolerance        		1e-6;
            relTol           		0.0;
            smoother         		GaussSeidel;
            nPreSweeps       		0;
            nPostSweeps      		2;
            cacheAgglomeration 	on;
            agglomerator     		faceAreaPair;
            nCellsInCoarsestLevel 	100;
            mergeLevels      		1;
            minIter		 	1;       
      }
      "(U|k|epsilon|omega|R|f|v2)"
      {
            solver                  PBiCGStab;
            preconditioner          DILU;
            tolerance               1e-5;       // 1e-08; wolfDynamics // original de twoWay 5e-8
            relTol                  0;          // 0; wolfDynamics    
            minIter			1;              
      }

